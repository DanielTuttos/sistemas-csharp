<?xml version="1.0" standalone="yes"?>
<xs:schema id="invoiceSet" targetNamespace="http://www.tempuri.org/invoiceSet.xsd" xmlns:mstns="http://www.tempuri.org/invoiceSet.xsd" xmlns="http://www.tempuri.org/invoiceSet.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="invoiceSet" msdata:IsDataSet="true">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element name="Invoice">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Address" type="xs:string" minOccurs="0" />
              <xs:element name="AmountDue" type="xs:decimal" minOccurs="0" />
              <xs:element name="AmountPaid" type="xs:decimal" minOccurs="0" />
              <xs:element name="AmpherName" type="xs:string" minOccurs="0" />
              <xs:element name="ArrivalDate" type="xs:dateTime" minOccurs="0" />
              <xs:element name="CRate" type="xs:double" minOccurs="0" />
              <xs:element name="CustomerID" type="xs:int" minOccurs="0" />
              <xs:element name="CustomerName" type="xs:string" minOccurs="0" />
              <xs:element name="Description" type="xs:string" minOccurs="0" />
              <xs:element name="DRate" type="xs:double" minOccurs="0" />
              <xs:element name="DriverCommision" type="xs:decimal" minOccurs="0" />
              <xs:element name="DriverName" type="xs:string" minOccurs="0" />
              <xs:element name="Insurance" type="xs:decimal" minOccurs="0" />
              <xs:element name="InvoiceID" msdata:AutoIncrement="true" type="xs:int" />
              <xs:element name="LRate" type="xs:double" minOccurs="0" />
              <xs:element name="PaymentType" type="xs:string" minOccurs="0" />
              <xs:element name="Price" type="xs:decimal" minOccurs="0" />
              <xs:element name="ProductID" type="xs:int" minOccurs="0" />
              <xs:element name="ProductName" type="xs:string" minOccurs="0" />
              <xs:element name="ProvinceName" type="xs:string" minOccurs="0" />
              <xs:element name="ShippingDate" type="xs:dateTime" minOccurs="0" />
              <xs:element name="TotalCost" type="xs:decimal" minOccurs="0" />
              <xs:element name="Weight" type="xs:int" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
    <xs:unique name="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:Invoice" />
      <xs:field xpath="mstns:InvoiceID" />
    </xs:unique>
  </xs:element>
</xs:schema>