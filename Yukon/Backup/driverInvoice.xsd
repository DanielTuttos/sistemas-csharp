<?xml version="1.0" standalone="yes"?>
<xs:schema id="driverInvoice" targetNamespace="http://www.tempuri.org/driverInvoice.xsd" xmlns:mstns="http://www.tempuri.org/driverInvoice.xsd" xmlns="http://www.tempuri.org/driverInvoice.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="driverInvoice" msdata:IsDataSet="true">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element name="Driver">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Address" type="xs:string" minOccurs="0" />
              <xs:element name="Commision" type="xs:decimal" minOccurs="0" />
              <xs:element name="DriverID" msdata:AutoIncrement="true" type="xs:int" />
              <xs:element name="FirstName" type="xs:string" minOccurs="0" />
              <xs:element name="LastName" type="xs:string" minOccurs="0" />
              <xs:element name="Salary" type="xs:decimal" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
    <xs:unique name="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:Driver" />
      <xs:field xpath="mstns:DriverID" />
    </xs:unique>
  </xs:element>
</xs:schema>